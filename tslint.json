{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "member-access": false,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ]
  },
  "rulesDirectory": []
}
